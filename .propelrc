{
  "type": "express",
  "templates": {
    "directory": ".templates",
    "model": {
      "replace": "MODEL",
      "destination": "app/models"
    },
    "controller": {
      "replace": "CONTROLLER",
      "destination": "app/controllers"
    },
    "middleware": {
      "replace": "MIDDLEWARE",
      "destination": "app/middleware"
    },
    "helper": {
      "replace": "HELPER",
      "destination": "app/helpers"
    }
  }
}
